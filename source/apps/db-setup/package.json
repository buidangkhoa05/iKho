{
  "name": "db-setup",
  "private": true,
  "scripts": {
    "dev": "go run .",
    "build": "go build -o ./dist/db-setup .",
    "test": "go test ./...",
    "lint": "go vet ./...",
    "fmt": "gofmt -l -w .",
    "tidy": "go mod tidy",
    "migrate:up": "go run . migrate up",
    "migrate:down": "go run . migrate down",
    "migrate:status": "go run . migrate status",
    "generate:model": "go run . generate",
    "dump": "go run . dump",
    "dump:data": "go run . dump --schema-only=false"
  }
}